{
  "name": "draughtsground",
  "version": "7.6.0",
  "description": "lidraughts.org Draughts UI",
  "type": "module",
  "module": "dist/draughtsboard.js",
  "types": "dist/draughtsground.d.ts",
  "directories": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "^7",
    "prettier": "^2",
    "tslib": "^2",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "prepare": "tsc",
    "compile": "tsc",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dev": "tsc",
    "prod": "tsc"
  },
  "playstrategy": {
    "bundles": {
      "src/draughtsground.ts": "draughtsground"
    }
  },
  "files": [
    "/*.js",
    "/*.d.ts",
    "/*.js.map",
    "/assets/*.css",
    "/src/*.ts"
  ],
  "keywords": [
    "draughts",
    "lidraughts.org",
    "draughtsboard",
    "UI",
    "typescript"
  ],
  "author": "Stefan Schermann (modified original by Thibault Duplessis)",
  "license": "GPL-3.0"
}
