{
  "name": "draughtsground",
  "version": "7.6.0",
  "description": "lidraughts.org Draughts UI",
  "type": "commonjs",
  "main": "draughtsground.js",
  "typings": "draughtsground.d.ts",
  "exports": {
    ".": "./dist/draughtsground.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "^7",
    "prettier": "^2",
    "tslib": "^2"
  },
  "scripts": {
    "prepare": "tsc",
    "compile": "tsc",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dev": "tsc",
    "prod": "tsc"
  },
  "files": [
    "/dist/*.js",
    "/dist/*.d.ts",
    "/dist/*.js.map",
    "/assets/*.css",
    "/src/*.ts"
  ],
  "keywords": [
    "draughts",
    "lidraughts.org",
    "draughtsboard",
    "UI",
    "typescript"
  ],
  "author": "Stefan Schermann (modified original by Thibault Duplessis)",
  "license": "GPL-3.0",
  "playstrategy": {
    "bundles": "src/draughtsground.ts"
  }  
}
