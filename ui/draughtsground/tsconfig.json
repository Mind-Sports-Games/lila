{
  "extends": "../tsconfig_module.base.json",
  "include": ["src/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "src",
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "esnext",
    "target": "ES2019",
    "lib": ["DOM", "ES2019"],
    "types": [],
    "composite": true
  },
  "isolatedModules": true
}