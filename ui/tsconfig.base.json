{
  "compilerOptions": {
    "strict": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "target": "es2019",
    "module": "esnext",
    "lib": ["es2019"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "types": ["playstrategy", "web"],
    "noImplicitAny": false
  }
}
