{
  "compilerOptions": {
    "strict": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "target": "es2017",
    "module": "esnext",
    "lib": ["es2017"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "types": ["playstrategy", "web"]
  }
}
